ssh ubuntu@10.42.0.1

cd ~/turtlebot3_ws/ && colcon build --symlink-install

source install/setup.bash && source install/setup.sh && source /opt/ros/humble/setup.sh && source /usr/share/gazebo/setup.sh

Raspberry Pi:

export ROS_DOMAIN_ID=8  # Optional, but helps to prevent network collisions
export TURTLEBOT3_MODEL=burger

ros2 launch turtlebot3_bringup robot.launch.py

ros2 run camera_ros camera_node --ros-args -p format:=BGR888 -p width:=640 -p height:=480

Remote PC:

export ROS_DOMAIN_ID=8  # Ensure this matches with the TurtleBot3
export TURTLEBOT3_MODEL=burger

ros2 run turtlebot3_teleop teleop_keyboard

ros2 launch turtlebot3_cartographer cartographer.launch.py use_sim_time:=True

ros2 run turtlebot3_gazebo CStockList

ros2 run turtlebot3_gazebo CArUcoDetector

ros2 run turtlebot3_gazebo CDriveLogic

ros2 launch turtlebot3_gazebo turtlebot3_bringup.launch.py use_sim_time:=true
